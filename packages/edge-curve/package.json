    "url": "http://github.com/jacomyal/sigma.js.git",
    "directory": "packages/edge-curve"
  },
  "keywords": [
    "graph",
    "graphology",
    "sigma"
  ],
  "contributors": [
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    },
    {
      "name": "Alexis Jacomy",
      "url": "http://github.com/jacomyal"
    }
  ],
  "license": "MIT",
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "peerDependencies": {
