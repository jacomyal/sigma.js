{
  "version": 2,
  "builds": [
    {
      "src": "packages/demo/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/packages/demo/build/index.html" }
  ]
} 